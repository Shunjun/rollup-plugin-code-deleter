{
  "name": "rollup-plugin-code-deleter",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@8.12.1",
  "description": "A rollup plugin to delete code by Comments",
  "author": "Shunzi <tobyzsj@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/Shunjun/rollup-plugin-code-deleter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shunjun/rollup-plugin-code-deleter.git"
  },
  "bugs": "https://github.com/Shunjun/rollup-plugin-code-deleter/issues",
  "keywords": [
    "rollup",
    "rollup-plugin",
    "code-deleter",
    "delete-code",
    "rollup-plugin-code-deleter"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@babel/core": "^7.23.9",
    "@babel/traverse": "^7.23.9",
    "@babel/types": "^7.23.9",
    "debug": "^4.3.4",
    "minimatch": "^9.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.2",
    "@types/babel__core": "^7.20.5",
    "@types/babel__traverse": "^7.20.5",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.10.7",
    "bumpp": "^9.2.1",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.0",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.11",
    "vitest": "^1.1.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
